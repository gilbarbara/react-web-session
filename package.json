{
  "name": "react-web-session",
  "version": "0.2.3",
  "description": "Keep session data between windows",
  "repository": {
    "type": "git",
    "url": "git://github.com/gilbarbara/react-web-session.git"
  },
  "keywords": [
    "react",
    "react-component",
    "component",
    "session"
  ],
  "author": "Gil Barbara <gilbarbara@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gilbarbara/react-web-session/issues"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "es",
    "lib",
    "src"
  ],
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "prop-types": "^15.0.0",
    "history": "^4.0.0"
  },
  "dependencies": {
    "web-session": "^0.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.4.2",
    "babel-plugin-add-module-exports": "^1.0.0",
    "chalk": "^2.4.1",
    "cross-env": "^5.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "history": "^4.7.2",
    "husky": "^1.3.0",
    "jest": "^23.6.0",
    "jest-environment-jsdom-global": "^1.1.0",
    "jest-enzyme": "^7.0.1",
    "jest-localstorage-mock": "^2.3.0",
    "lolex": "^3.0.0",
    "luxon": "^1.8.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "yargs": "^12.0.5"
  },
  "scripts": {
    "build": "npm run clean && npm run build:commonjs && npm run build:es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src -d lib",
    "build:es": "cross-env NODE_ENV=production babel src -d es",
    "watch": "npm run build:commonjs -- -w",
    "clean": "rimraf es && rimraf lib",
    "lint": "eslint --ext .js --ext .jsx src test",
    "test": "TZ=UTC jest --coverage",
    "test:watch": "TZ=UTC jest --watch",
    "prepublishOnly": "npm run build"
  },
  "husky": {
    "hooks": {
      "post-merge": "node tools update",
      "pre-commit": "node tools upstream && npm run validate"
    }
  }
}
